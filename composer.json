{
    "name": "sfneal/view-export",
    "description": "Export Views from Laravel applications to PDF or Excel files",
    "keywords": [
        "sfneal",
        "view-export"
    ],
    "homepage": "https://github.com/sfneal/view-export",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "dompdf/dompdf": "0.8.5",
        "laravel/framework": "^8.8",
        "maatwebsite/excel": "^3.1",
        "sfneal/actions": "^0.1.3",
        "sfneal/aws-s3-helpers": "^0.2.0",
        "sfneal/queueables": "^0.2.0",
        "sfneal/string-helpers": "^0.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\ViewExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\ViewExport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
